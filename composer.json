	"config" : {
		"optimize-autoloader" : true,
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true,
		"allow-plugins": {
			"symfony/flex": true
		}
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php72" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : false,
			"require" : "5.4.*"
		}
	}
}
