{% extends 'admin_base.html.twig' %}

{% block title %}EmailType{% endblock %}

{% block content %}
    <div class="container">
        <h1>EmailType</h1>
   
        <table class="table">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ email_type.id }}</td>
                </tr>
                <tr>
                    <th>Label</th>
                    <td>{{ email_type.label }}</td>
                </tr>
                <tr>
                    <th>Slug</th>
                    <td>{{ email_type.slug }}</td>
